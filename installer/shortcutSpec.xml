<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="SnackCoin" location="startMenu"/>
	<shortcut
		name="SnackCoin Server"
		target="$INSTALL_PATH\SnackCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start SnackCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="SnackCoin Wallet"
		target="http://localhost:7010/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch SnackCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>